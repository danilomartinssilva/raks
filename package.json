  "private": true,
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "start": "node dist/shared/infra/http/server",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify src/shared/infra/http/server.ts",
    "lint:fix": "eslint --fix src --ext ts,tsx",
    "release": "standard-version --prerelease",
    "pretest": "NODE_ENV=test sequelize db:migrate",
    "test": "NODE_ENV=test jest",
    "posttest": "NODE_ENV=test sequelize db:migrate:undo:all",    
    "test:watch": "yarn test --watch",
    "test:staged": "yarn test --passWithNoTests --findRelatedTests",
    "test:ci": "yarn test --coverage",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli"
  },
